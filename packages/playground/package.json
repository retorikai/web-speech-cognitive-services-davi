{
  "name": "playground",
  "version": "0.1.0",
  "homepage": "https://compulim.github.io/web-speech-cognitive-services",
  "private": true,
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "build": "react-scripts build",
    "bump": "npm install --legacy-peer-deps $(cat package.json | jq -r '((.dependencies, .devDependencies) | keys) - (.skipBump | keys) | .[]' | awk '{print $1 \"@latest\"}')",
    "clean": "npx rimraf build",
    "eject": "react-scripts eject",
    "prepublishOnly": "npm run build",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom"
  },
  "skipBump": {},
  "dependencies": {
    "@azure/identity": "^3.1.3",
    "@azure/ms-rest-azure-js": "^2.1.0",
    "@azure/ms-rest-browserauth": "^1.0.2",
    "classnames": "^2.3.2",
    "glamor": "^2.20.40",
    "microsoft-cognitiveservices-speech-sdk": "^1.26.0",
    "on-error-resume-next": "^1.1.0",
    "p-defer": "^4.0.0",
    "p-defer-es5": "^2.0.1",
    "react": "^17.0.2",
    "react-dictate-button": "^2.0.1",
    "react-dom": "^17.0.2",
    "react-redux": "^8.0.4",
    "react-say": "^2.1.0",
    "react-scripts": "^5.0.1",
    "redux": "^4.2.0",
    "redux-saga": "^1.2.1",
    "simple-update-in": "^2.2.0",
    "web-speech-cognitive-services": "file:.."
  },
  "devDependencies": {
    "webpack-stats-plugin": "^1.1.1"
  }
}
